{
  "name": "wys",
  "displayName": "Wys!syn",
  "description": "Wys! programming language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "wlang",
      "aliases": ["Wys!", "wlang"],
      "extensions": [".wys",".w",".wy",".wyh",".wysh",".wlib",".wjson",".wpack",".wdef"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "wlang",
      "scopeName": "source.wys",
      "path": "./syntaxes/wlang.tmLanguage.json"
    }]
  }
}
